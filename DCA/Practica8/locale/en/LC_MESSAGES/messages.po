#: i18n_ejemplo_v2.py:20
msgid "La máquina eligió piedra"
msgstr "The machine chose rock"

#: i18n_ejemplo_v2.py:22
msgid "La máquina eligió papel"
msgstr "The machine chose rock"

#: i18n_ejemplo_v2.py:24
msgid "La máquina eligió tijera"
msgstr "The machine chose rock"

#: i18n_ejemplo_v2.py:28
msgid "¿Piedra, papel o tijera?"
msgstr "Rock Paper Scissors?"

#: i18n_ejemplo_v2.py:29
msgid "1. Piedra"
msgstr "1. Rock"

#: i18n_ejemplo_v2.py:30
msgid "2. Papel"
msgstr "2. Paper"

#: i18n_ejemplo_v2.py:31
msgid "3. Tijera"
msgstr "3. Scissors"

#: i18n_ejemplo_v2.py:33
msgid "Elige una opción: "
msgstr "Choose one: "

#: i18n_ejemplo_v2.py:35
msgid "Elegiste piedra"
msgstr "You chose Rock"

#: i18n_ejemplo_v2.py:37
msgid "Elegiste papel"
msgstr "You chose Paper"

#: i18n_ejemplo_v2.py:39
msgid "Elegiste tijera"
msgstr "You chose Scissors"

#: i18n_ejemplo_v2.py:41
msgid "Opción inválida. Por favor, intente de nuevo."
msgstr "Invalid option. Please, try again"

#: i18n_ejemplo_v2.py:53
msgid "¡Empate!"
msgstr "It's a tie!"

#: i18n_ejemplo_v2.py:55 i18n_ejemplo_v2.py:57 i18n_ejemplo_v2.py:59
msgid "¡Ganaste!"
msgstr "You win!"

#: i18n_ejemplo_v2.py:61
msgid "¡Perdiste!"
msgstr "You lose!"
